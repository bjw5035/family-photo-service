{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run FastAPI (Uvicorn)",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "app.main:app",
        "--host",
        "0.0.0.0",
        "--port",
        "8000",
        "--reload"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "API_KEY": "dev-key"
      },
      "console": "integratedTerminal"
    }
  ]
}
